
<%@ page contentType="text/html; charset=UTF-8" %>
<!DOCTYPE html>
<html>
<head>
    <title>Colorful UI Example</title>
    <style>
        /* Define some colors */
        .red { color: red; }
        .blue { color: blue; }
        .green { color: green; }
        .yellow { color: yellow; }
    </style>
</head>
<body>
    <h1>Welcome to the Colorful UI Page!</h1>

    <%-- Use JSP scriptlets to dynamically apply different colors --%>
    <h2 class="red">This text is red!</h2>
    <h2 class="blue">This text is blue!</h2>
    <h2 class="green">This text is green!</h2>
    <h2 class="yellow">This text is yellow!</h2>
</body>
</html>
